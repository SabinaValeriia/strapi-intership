{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "image": {
      "type": "json"
    },
    "projects_manager": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project.project",
      "mappedBy": "manager"
    },
    "projects_members": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::project.project",
      "mappedBy": "members"
    },
    "tasks_asignee": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tasks.tasks",
      "mappedBy": "asignee"
    },
    "tasks_members": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tasks.tasks",
      "mappedBy": "members"
    },
    "tasks_reporter": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tasks.tasks",
      "mappedBy": "reporter"
    },
    "department": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::department.department",
      "inversedBy": "users_department"
    },
    "manager": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "user_managers"
    },
    "user_managers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "manager"
    },
    "project": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project.project",
      "mappedBy": "user_project"
    },
    "tasks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tasks.tasks",
      "mappedBy": "user_tasks"
    },
    "projects_lead": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project.project",
      "mappedBy": "lead"
    }
  }
}
